// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         Int      @id @unique
  email      String   @unique
  password   String
  idDiscord  String   @unique
  avatar_url String
  insertedAt DateTime
}
